{
  "name": "mog-render-service",
  "version": "0.0.0",
  "description": "Markdown & Djot Render Service for Mog Core",
  "author": "wibus-wee <wibus@qq.com>",
  "license": "AGPLv3",
  "homepage": "https://github.com/mogland-dev/mog-render-service#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mogland-dev/mog-render-service.git"
  },
  "issues": "https://github.com/mogland-dev/mog-render-service/issues",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache --fix"
    ]
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@innei-util/eslint-config-ts": "0.8.2",
    "@innei-util/prettier": "0.8.2",
    "@innei/eslint-config-ts": "0.9.8",
    "@types/marked": "^4.0.8",
    "@types/node": "18.15.8",
    "bumpp": "8.2.1",
    "eslint": "8.36.0",
    "esno": "0.16.3",
    "husky": "8.0.3",
    "lint-staged": "13.0.4",
    "pnpm": "7.17.1",
    "prettier": "2.8.7",
    "rimraf": "3.0.2",
    "typescript": "4.9.5",
    "unbuild": "1.0.2",
    "vite": "3.2.5",
    "vitest": "0.25.8"
  },
  "dependencies": {
    "@djot/djot": "^0.2.2",
    "chalk": "^5.2.0",
    "consola": "^2.15.3",
    "marked": "^4.3.0",
    "redis": "^4.6.5"
  }
}
